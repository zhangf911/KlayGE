<?xml version='1.0'?>

<gl_extension name="EGL_ANDROID_presentation_time" reg_no="98">
	<typedefs>
		<typedef type="khronos_stime_nanoseconds_t" synonym="EGLnsecsANDROID"/>
	</typedefs>

	<functions>
		<function name="eglPresentationTimeANDROID" return="EGLBoolean">
			<params>
				<param type="EGLDisplay" name="dpy"/>
